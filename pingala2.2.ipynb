    "from matplotlib import pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "x = [1, 1]\n",
    "for i in range(23): x.append(x[-1] + x[-2])\n",
    "y = np.add(x[:23], x[2:])\n",
    "plt.stem(range(23), y)\n",
    "plt.grid()\n",
    "plt.xlabel('$n$')\n",
    "plt.ylabel('$y(n)$')\n",
    "plt.tight_layout()\n",
    "#plt.savefig('../figs/2_2.png')"
   ]
